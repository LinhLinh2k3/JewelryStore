<script>
    const avatarImg = document.querySelector("#avatarReg"), input = document.querySelector("#upload");
    input.addEventListener("change", () => {
        avatarImg.src = URL.createObjectURL(input.files[0]);
    });

    $(function () {
        $('#deleteModal').on('show.bs.modal', function (event) {
            var button = $(event.relatedTarget); // Button that triggered the modal
            var url = button.attr('href'); // Extract the URL from the delete button
            var modal = $(this);
            modal.find('.btn-danger').attr('href', url); // Set the delete URL as the "href" of the delete button in the modal
        });
    });
</script>

@if (ViewBag.ToastMsg != null)
{
    <script>
        const toast = document.getElementById('toast');
        bootstrap.Toast.getOrCreateInstance(toast).show();

        //$(document).ready(function () {
        //    $('#toast').toast({ delay: 2000 });
        //    $('#toast').toast('show');
